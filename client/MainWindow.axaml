        <Button Name="StartServerButton" Content="Start server" HorizontalAlignment="Center" Click="StartServerButton_Click"/>
        <TextBox Name="MessagesTextBox" HorizontalAlignment="Stretch" Margin="0,10" IsReadOnly="True"/>
    </StackPanel>
</Window>
